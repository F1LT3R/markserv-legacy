{
  "name": "markserv",
  "version": "0.1.0",
  "description": "markserv serves Markdown files as GitHub style HTML and LiveReloads your files in the browser as you edit.",
  "keywords": [
    "markdown",
    "livereload",
    "server",
    "github"
  ],
  "scripts": {
    "test": "xo"
  },
  "bin": {
    "markserv": "server.js"
  },
  "author": "Alistair MacDonald <al@pwn.io>",
  "email": "al@pwn.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/f1lt3r/markserv.git"
  },
  "dependencies": {
    "ansi": "^0.3.0",
    "bluebird": "^3.4.7",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.5.1",
    "commonmark": "^0.28.1",
    "connect": "^3.6.5",
    "connect-livereload": "^0.6.0",
    "jsdom": "^9.9.1",
    "less": "^2.7.2",
    "livereload": "^0.6.3",
    "livereload-js": "^2.2.2",
    "lunr": "^2.1.4",
    "micromatch": "^3.1.3",
    "mime": "^1.4.1",
    "open": "0.0.5",
    "openport": "0.0.4",
    "pretty": "^2.0.0",
    "send": "^0.16.0",
    "snyk": "^1.26.1",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "xo": "^0.17.0"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "document",
      "io",
      "var"
    ],
    "rules": {
      "no-use-extend-native": 0
    }
  },
  "snyk": true
}
